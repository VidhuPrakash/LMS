{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": false,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/": ["./src/"]
    },
    "outDir": "./dist",
    "types": ["bun-types"],
    "composite": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  }
}